<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="git" parallel="tests" thread-count="2">
	<listeners>
		<listener class-name="com.Listeners.Listeners" />
	</listeners>
	<test name="AlumniPage_Confirmation">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.Cases.AlumniCases" />
		</classes>
	</test>
	<test name="Student_Admission">
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="com.Cases.AdmissionCases" />
		</classes>
	</test>
	<test name="Student_SignIn">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.Cases.SignInCases" />
		</classes>
	</test>
	<test name="Online_Payment">
		<parameter name="browser" value="edge" />
		<classes>
			<class name="com.Cases.PaymentCases" />
		</classes>
	</test>
</suite>
